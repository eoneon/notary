<% user.jobs.each do |job| %>
  <%= content_tag :div, class: "media js-jobs", id: "job-#{job.id}" do %>
    <div class="media">
      <div class="well">
        <p><%= link_to job.job_date, user_job_path(job.user, job) %>
        <p><%= link_to "Edit job", edit_user_job_path(job.user, job) %> |
           <%= link_to "Delete job", [job.user, job], method: :delete, remote: true, data: { confirm: 'Are you sure you want to delete this job?' } %>
        </p>
      </div>
    </div>
  <% end %>
<% end %>
