<h1>Jobs</h1>
<h4 class="media-heading">Create Job <%= link_to "New Job", new_job_path, class: 'btn btn-success' %></h4>
<div class="row">
 <div class="col-md-8">
   <% @jobs.each do |job| %>
     <div class="media">
       <div class="media-body">
         <h4 class="media-heading">
           <%= link_to job.job_date, job %> | <%= job.signers_last_name_first if job.people.first %> | <%= job.billable.name if job.type == "PackageJob" && job.billable %>
         </h4>
       </div>
     </div>
   <% end %>
 </div>
</div>
